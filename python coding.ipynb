#count prime numbers
def prime_number(n):
    if n < 2:
        return False
    for i in range(2, int(n**0.5) + 1):
        if n % i == 0:
            return False
    return True

def count_primes(n):
    count = 0
    for i in range(1, n+1):
        if prime_number(i):
            count += 1
    return count

n = int(input("Enter N: "))
print("Total primes:", count_primes(n))

output:
Enter Number: 20
Total prime number: 8

